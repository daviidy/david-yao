<!DOCTYPE html>
<html>
  <head>
    <title>David Yao</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if flash[:notice] %>
      <strong style="color: green;"><%= notice %></strong>
    <% end %>
    <%= yield %>
  </body>
</html>
